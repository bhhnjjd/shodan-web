{
  "name": "shodan-mapper",
  "version": "1.0.0",
  "description": "Comprehensive network mapping website using Shodan API",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.0",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.0",
    "node-cache": "^5.1.2",
    "dotenv": "^16.3.1",
    "validator": "^13.11.0",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "concurrently": "^8.2.0"
  },
  "keywords": ["shodan", "network-mapping", "cybersecurity", "vulnerability-analysis"],
  "author": "Network Security Tools",
  "license": "MIT"
}